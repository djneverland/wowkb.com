.button {
	width:210px;
	height:35px;
	background:#009ee7;
	color:#fff;
	border:0;
	margin-left:145px;
	margin-top:15px;
	font-size:1.4em;
	font-weight:bold
}
select {
	width:210px;
	height:30px;
	padding:5px;
	border:1px solid #ccc
}
.article h1{
	border-bottom:4px solid #eee;
}
.article .entry {
	width:125px;
	float:left
}
.article p{
	clear:both;
	overflow:hidden;
	line-height:28px;
	*line-height:30px;
	_line-height:30px;
	*margin-top:10px;
	*margin-bottom:10px;
	_margin-top:10px;
	_margin-bottom:10px
}
.article p.special {
    padding-left: 10px
}
.article .red {
	color:red;
	margin-right:5px;
	line-height:26px
}
.article input.general {
	width:200px;
	height:26px;
	line-height:26px;
	padding-left:5px
}
.article .grtextarea {
	width:400px;
	height:60px;
	padding:5px;
	padding-left:0
}
.article span.fill{
	color:red;
	float:left;
	margin-right:7px;
	line-height:30px
}
.aside ul{
	*padding:0;
	_padding:0;
	*margin-top:10px;
	_margin_top:10px
}